<FindBugsFilter>

  <!-- exclude messages with a rank lower than 15. Ranks range from 1 to 20, where 1 is top and 20 is lowest. -->
  <Match>
    <Rank value="16" />
  </Match>

  <!-- Temporary disabled due to potential false positive reported in https://github.com/spotbugs/spotbugs/issues/1923. -->
  <Bug code="NP" />

</FindBugsFilter>